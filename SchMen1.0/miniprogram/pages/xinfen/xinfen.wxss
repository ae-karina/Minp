  .add-page{
    width: 100%;
    /* font-size: 43rpx; */
  }
  .feedback-block{
    margin: 0 15px;
    background-color: #ffffff;
    /* background-color: rgba(65, 40, 40, 0.1); */
  }
  .im{
    width: 60rpx;
    height: 60rpx;
    margin-right: 5px;
  }
  .feedback-title{
    display: flex; 
    width: 100%;
    height: 98rpx;
    align-items: center;
    line-height: 98rpx;
    font-weight: bold;
    color: #CC9966;
    border-bottom:1rpx solid #f1f1f1;
    /* margin-left: 23rpx; */
  }
  .feedback-con{
    height: 300rpx;
    padding: 10px 5px;
    /* background-color: rgba(143, 116, 206, 0.5); */
  }
  .feedback-detail{
    width: 100%;
    height: 100%;
    padding-left: 3px;
    font-size: 16px;
    white-space: pre-wrap;
    /* background-color: rgba(143, 116, 206, 0.5); */
  }
  .img-wrap {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin: 0 35rpx;
    padding: 10rpx 0;
    /* border-top:1rpx solid #f1f1f1; */
  }
  .one-img {
    position: relative;
    margin: 4rpx 8rpx;
  }
  .img-choose {
    width: 140rpx;
    height: 140rpx;
    border: 1px solid #f1f1f1;
    background-color:#ffffff;
  }
  .close-img {
    position: absolute;
    top: 4rpx;
    right: 4rpx;
    width: 30rpx;
    height: 30rpx;
  }
  .picker{
    margin-top: 8px;
    color: rgb(148, 139, 139);
  }
  .bottom-btn{
    margin-top: 60rpx;
  }
  .btn{
    width: 120px !important;
    color: #fff;
    border-radius: 25px;
    background-color: #b3d869;
  }
