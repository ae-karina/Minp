

page{
    padding-bottom: 200rpx;
  }

  .add-page{
    width: 100%;
    font-size: 43rpx;
   
    
  }
  .address-block{
    padding-left: 10rpx;
    padding-right: 51rpx;
    background-color: #ffffff;
  }
  .address-info{
    width: 100%;
    display: flex;
    justify-content: space-between;
    height: 98rpx;
    line-height: 98rpx;
    border-bottom:1rpx solid #f1f1f1;
   
  }
  .address-info:last-child{
    border-bottom:none;
  }
  .address-info input{
    height: 100%;
  }
  .address-title{
    flex: 0 0 130rpx;
    text-align:justify;
  }
  .address-title:after{
    display:inline-block;
    content:'';
    width:100%;
  }
  .address-content{
    flex: 1 1 auto;
    height: 100%;
    padding-left: 35rpx;
    border-top:1rpx solid #f1f1f1;
   
  }
  .address-region{
    display: inline-flex;
    justify-content: space-between;
    position: relative;
  }
  .address-region-name{
    width: 450rpx;
  }
  .address-region .arrow-right{
    display: inline-block;
    position: absolute;
    right: 0;
    top: 40rpx;
  }
  .address-detail{
    height: 336rpx;
    padding-top: 10rpx;
    line-height: 41rpx;
  }
  .address-detail-text{
    width: 100%;
    height: 100%;
  }
   


  .img-wrap {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin: 10rpx 35rpx;
  }
  .one-img {
    position: relative;
    margin: 4rpx 8rpx;
  }
  .img-choose {
    width: 140rpx;
    height: 140rpx;
    border: 1px solid #f1f1f1;
    background-color:#ffffff;
  }
  .close-img {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    right: 4rpx;
    top: 4rpx;
  }
  .choose-localtion {
    margin: 10rpx 35rpx;
    font-size: 43rpx;
    color: rgb(0, 0, 0);
    padding-bottom: 30rpx;
   
  }

  .address-infop{
    width: 100%;
    display: flex; 
    align-items: center;
    height: 98rpx;
    line-height: 98rpx;
    font-weight: bold;
  
    color: #CC9966;
    margin-left: 23rpx;
    
    
  }

  .bottom-btn{
    margin-top: 60rpx;
}

.im{
  width: 60rpx;
  height: 60rpx;
}